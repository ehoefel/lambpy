TRUE = λx.λy.x
FALSE = λx.λy.y
AND = λp.λq.p q p
OR = λp.λq.p p q
NOT = λp.p FALSE TRUE
SUCC = λn.λf.λx.f(n f x)
0 = λf.λx.x
1 = λf.λx.f x
2 = λf.λx.f (f x)
ADD = λm.λn.m SUCC n
MULT = λm.λn.m (ADD n) 0
POWER = λb.λe.e b
PRED = λn.λf.λx.n (λg.λh.h (g f)) (λu.x) (λu.u)
SUB = λm.λn.n PRED m
is0 = λn.n (λx.FALSE) TRUE
LEQ = λm.λn.is0 (SUB m n)
PAIR = λx.λy.λf.f x y
1st = λp.p TRUE
2nd = λp.p FALSE
